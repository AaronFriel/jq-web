{
  "name": "jq-web",
  "version": "0.0.3",
  "description": "a hack that makes jq run in the browser with emscripten.",
  "main": "jq.min.js",
  "scripts": {
    "wrap": "browserify --standalone jq jq.raw.js -o jq.js",
    "wrap-wasm": "browserify --standalone jq jq.wasm.raw.js -o jq.wasm.js",
    "minify": "uglifyjs jq.js -m -c -o jq.min.js",
    "minify-wasm": "uglifyjs jq.wasm.js -m -c -o jq.wasm.min.js",
    "prepublish": "npm run wrap && npm run minify && npm run wrap-wasm && npm run minify-wasm",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiatjaf/jq-web.git"
  },
  "keywords": [
    "emscriptem",
    "jq",
    "json",
    "filter",
    "path"
  ],
  "author": "Giovanni T. Parra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fiatjaf/jq-web/issues"
  },
  "homepage": "https://github.com/fiatjaf/jq-web#readme",
  "devDependencies": {
    "browserify": "^14.3.0",
    "tape": "^4.6.3",
    "uglify-js": "^2.8.22"
  }
}
